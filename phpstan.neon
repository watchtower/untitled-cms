includes:
    - vendor/larastan/larastan/extension.neon

parameters:
    paths:
        - app/
        - config/
        - routes/

    # The level 9 is the highest level
    level: 5

    ignoreErrors:
        - '#PHPDoc tag @var#'

    excludePaths:
        - app/Http/Middleware/Authenticate.php (?)
        - app/Http/Middleware/RedirectIfAuthenticated.php (?)